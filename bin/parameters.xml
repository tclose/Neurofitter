<TestProgram>
	<Dimensions>2</Dimensions>	
	<VerboseLevel>5</VerboseLevel>
	<Seed>1550</Seed>
	<SamplingFrequency>5000</SamplingFrequency>
	<StartingPoints>
		1.0 1.0
	</StartingPoints>
	<Bounds>
		1.0 100000.0 
		1.0 100000.0
	</Bounds>

	
	<Model type="Genesis">
		<GenesisLocation>/usr/local/bin/nxgenesis</GenesisLocation>
		
		<ModelDirectory>../../wernermodel</ModelDirectory>
		<ModelSource>wernermodel.g</ModelSource>
		
		<ParamFilePrefix>param</ParamFilePrefix>
		<OutputFilePrefix>output</OutputFilePrefix>
		
		<ErrorFile>generr.out</ErrorFile>
		
		<NumberOfRuns>2</NumberOfRuns>
		<NumberOfRunParameters>1</NumberOfRunParameters>
		<RunParameters>
			0 0.33
			0.1 0.33
			0.4 0.33
		</RunParameters>
		
		<NumberOfPeriods>2</NumberOfPeriods>
		<Periods>
			1.0 1.5 0.5
			2.0 3.9 0.5
		</Periods>
		
		<NumberOfRecordSites>1</NumberOfRecordSites>
		<RecordSites>
			1
		</RecordSites>
	</Model>	
	
	<Experiment type="Fake">
		<Parameters>2800.0 11200.0</Parameters>	
	</Experiment>
	
	<FitnessCalculator type="Pablo">
		<PabloVdVdtMatrix>
			<vLength>100</vLength>
			<dVdtLength>100</dVdtLength>
			<minimalV>-0.1</minimalV>
			<maximalV>0.1</maximalV>
			<comparePrecision>1e-15</comparePrecision>
		</PabloVdVdtMatrix>
		<enableFileExport>1</enableFileExport>
		<exportFile>fitness.dat</exportFile>
	</FitnessCalculator>
	
	<Fitter type="NOMAD">
		<INITIAL_POLL_SIZE>1000</INITIAL_POLL_SIZE>
		<MAX_POLL_SIZE>1000</MAX_POLL_SIZE>
		<POLL_BASIS>4</POLL_BASIS> 
		<COARSENING_EXPONENT>2</COARSENING_EXPONENT>
		<REFINING_EXPONENT>-2</REFINING_EXPONENT>
		
		<POLL_ORDER>0</POLL_ORDER>
		<POLL_COMPLETE>0</POLL_COMPLETE>
		<POLL_DIRECTIONS>4</POLL_DIRECTIONS>
		
		<INITIAL_SEARCH>0</INITIAL_SEARCH>
		<INITIAL_COMPLETE>0</INITIAL_COMPLETE>
		<INITIAL_POINTS>4</INITIAL_POINTS>
		
		<ITERATIVE_SEARCH>0</ITERATIVE_SEARCH>
		<ITERATIVE_COMPLETE>0</ITERATIVE_COMPLETE>
		<ITERATIVE_POINTS>0</ITERATIVE_POINTS>
		
		<SPECULATIVE_SEARCH>1</SPECULATIVE_SEARCH>  1
		
		<POLL_SIZE_TERM>-1</POLL_SIZE_TERM>    
		<CONSECUTIVE_FAILS>-1</CONSECUTIVE_FAILS>
		<TRUTH_EVALS>100</TRUTH_EVALS>
		<ITERATIONS>-1</ITERATIONS>
		<NEW_TRUTH_EVALS>-1</NEW_TRUTH_EVALS>
		
		<FILTER_RELATIVE>1</FILTER_RELATIVE>
		<HMAX>50000</HMAX>
		<HMIN>0.0001</HMIN>
		<FILTER_NORM>2</FILTER_NORM>
		
		<SURROGATE_TOLERANCE>0</SURROGATE_TOLERANCE>
		
		<SCALING_CHOICE>0</SCALING_CHOICE>
		<GEN_CONS_NB>0</GEN_CONS_NB>
		<USE_SURROGATE>0</USE_SURROGATE>
		
		<START_PT_FILE>start_pt.txt</START_PT_FILE>
		<BOUNDS_FILE>bounds.txt</BOUNDS_FILE>
		<INPUT_FILE>input.txt</INPUT_FILE>
		<RESULTS_FILE>results.txt</RESULTS_FILE>
	</Fitter>	

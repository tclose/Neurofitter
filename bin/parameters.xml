<TestProgram>
	<Dimensions>2</Dimensions>	
	<VerboseLevel>5</VerboseLevel>
	<Seed>1550</Seed>
	<SamplingFrequency>5000</SamplingFrequency>
	<StartingPoints>
		1.0 1.0
	</StartingPoints>
	<Bounds>
		1.0 100000.0 
		1.0 100000.0
	</Bounds>
	
	<PabloFitter>
		<EOFitter>
			<EOParam>
					<![CDATA[
	
						]]>
			</EOParam>
		</EOFitter>
		<NOMADFitter>
			<parameters.dat>
				<![CDATA[
						#MESH PARAMETERS
						#---------------
						INITIAL_POLL_SIZE   1
						MAX_POLL_SIZE       100000
						
						POLL_BASIS      4 
						COARSENING_EXPONENT 1
						REFINING_EXPONENT   -1
						#
						#POLL PARAMETERS
						#---------------
						POLL_ORDER      0
						POLL_COMPLETE       0
						POLL_DIRECTIONS     4
						#
						#SEARCH PARAMETERS
						#-----------------
						INITIAL_SEARCH      0
						INITIAL_COMPLETE    0
						INITIAL_POINTS      4
						ITERATIVE_SEARCH    0
						ITERATIVE_COMPLETE  0
						ITERATIVE_POINTS    0
						SPECULATIVE_SEARCH  1
						#
						#TERMINATION PARAMETERS
						#----------------------
						POLL_SIZE_TERM      -1
						CONSECUTIVE_FAILS   -1
						TRUTH_EVALS     500
						ITERATIONS      -1
						NEW_TRUTH_EVALS     -1
						#
						#FILTER PARAMETERS
						#-----------------	
						FILTER_RELATIVE     1
						HMAX            50000
						HMIN            0.0001
						FILTER_NORM     2
						#
						#SURROGATE PARAMETERS                                                                                   
						#--------------------
						SURROGATE_TOLERANCE 0
						]]>
			</parameters.dat>
			<description.dat>
				<![CDATA[
						PROBLEM_NAME    PC
						SCALING_CHOICE  0
						GEN_CONS_NB 0
						USE_SURROGATE   0
						USE_BLACK_BOXES 0
						USE_BOUNDS  1
						USE_CACHES  1
						CACHE_FILE cache
						DIRECTORY .
						START_PT_FILE start_pt.txt
						RESULTS_FILE results.txt
						BOUNDS_FILE bounds.txt
						]]>
			</description.dat>
			<preferences.dat>
					<![CDATA[
							#PREFERENCES
							#-----------
							DISPLAY_FACTOR      5
							SEND_EMAIL      0
							]]>
				</preferences.dat>
		</NOMADFitter>	
	</PabloFitter>
	
	<MeshFitter>
		<MeshDimensions>2</MeshDimensions>
		<MeshSize>
			10
			10
		</MeshSize>
	</MeshFitter>
	
	<GenesisModel>
		<GenesisLocation>/usr/local/bin/nxgenesis</GenesisLocation>
		
		<ModelDirectory>../../wernermodel</ModelDirectory>
		<ModelSource>wernermodel.g</ModelSource>
		
		<ParamFilePrefix>param</ParamFilePrefix>
		<OutputFilePrefix>output</OutputFilePrefix>
		
		<ErrorFile>generr.out</ErrorFile>
		
		<NumberOfRuns>1</NumberOfRuns>
		<NumberOfRunParameters>1</NumberOfRunParameters>
		<RunParameters>
			0 0.33
			0.1 0.33
			0.4 0.33
		</RunParameters>
		
		<NumberOfPeriods>2</NumberOfPeriods>
		<Periods>
			1.0 1.5 0.5
			2.0 3.9 0.5
		</Periods>
		
		<NumberOfRecordSites>1</NumberOfRecordSites>
		<RecordSites>
			1
		</RecordSites>
	</GenesisModel>	
	
	<FakeExperiment>
			<Parameters>2800.0 11200.0</Parameters>	
	</FakeExperiment>
	
	<PabloFitnessCalculator>
		<PabloVdVdtMatrix>
			<vLength>100</vLength>
			<dVdtLength>100</dVdtLength>
			<minimalV>-0.1</minimalV>
			<maximalV>0.1</maximalV>
			<comparePrecision>1e-15</comparePrecision>
		</PabloVdVdtMatrix>
		<enableFileExport>1</enableFileExport>
		<exportFile>fitness.dat</exportFile>
	</PabloFitnessCalculator>
</TestProgram>
